	Url = {http://bioinformatics.oxfordjournals.org/content/19/1/79.abstract},
	Volume = {19},
	Year = {2003},
	Bdsk-Url-1 = {http://bioinformatics.oxfordjournals.org/content/19/1/79.abstract},
