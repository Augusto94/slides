	Number = {2},
	Pages = {121--132},
	Pmid = {16473279},
	Title = {{X chromosomal abnormalities in basal-like human breast cancer}},
	Volume = {9},
	Year = {2006},
	Bdsk-Url-1 = {http://dx.doi.org/10.1016/j.ccr.2006.01.013}}

